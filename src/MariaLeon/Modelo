@startuml

Class UnidadMilitar {
    -String codigo;
    -String nombre;
    -String descripcion;
    -Date fechaCreacion;
    -String direccion;
    -String objetivo;
    -String mision;
    -Soldado soldadoDirigente;
    -String tipoUnidad;
    -List<UnidadMilitar> subUnidades;

    +obtenerDetalles(): String
}

Class Cuartel {
    -String codigo;
    -String nombre;
    -String direccion;
    -int capacidad;
    -List<UnidadMilitar> unidadesAlojadas;
}

Class Soldado {
    -DocumentoIdentidad documentoIdentidad;
    -String codigoMilitar;
    -String nombre;
    -String apellido;
    -Date fechaNacimiento;
    -int edad;
    -String genero;
    -double alturaInicial;
    -double pesoInicial;
    -String direccion;
    -String telefono;
    -String email;
    -Rango rango;
    -CuerpoMilitar cuerpoMilitar;
    -Soldado superior;
    -List<ContactoEmergencia> contactosEmergencia;
    -List<UnidadMilitar> unidadesAsignadas;

    +obtenerDetalles(): String;
}

Class Rango {
    -String codigo;
    -String nombre;
    -String descripcion;
    -String insignia;
    -String linea;
    -int tiempoMinAscenso;
    -List<CriterioAscenso> criteriosAscenso;
    -CuerpoMilitar cuerpoMilitar;
    -Rango rangoSuperior;
}

Class CuerpoMilitar {
    -String codigo;
    -String nombre;
    -String descripcion;
    -String estructuraMando;
}

Class DocumentoIdentidad {
    -String numero;
    -String tipo;
    -Date fechaExpedicion;
    -String lugarExpedicion;
    -String tipoSangre;
}

Class ContactoEmergencia {
    -String numeroIdentificacion;
    -String nombre;
    -String apellido;
    -String genero;
    -String email;
    -String telefono;
    -Date fechaNacimiento;
    -String parentesco;
}

Class AsignacionComponente {
    -Soldado soldado;
    -UnidadMilitar unidadMilitar;
    -Date fechaIngreso;
    -String oficioInicial;
}

Class Labor {
    -String codigo;
    -String nombre;
    -String descripcion;
    -Date fechaAsignacion;
    -Date fechaInicio;
    -Date fechaFin;
    -List<Funcion> funciones;
}

Class Funcion {
    -String codigo;
    -String nombre;
    -String descripcion;
    -String procedimiento;
}

Class ProgramaEntrenamiento {
    -String codigo;
    -String nombre;
    -String descripcion;
    -List<Actividad> actividades;
    -Soldado soldadoResponsable;
    -Date fechaInicio;
    -Date fechaFin;

    +obtenerActividadesProgramadas(): List<Actividad>
}

Class Actividad {
    #String codigo;
    #String nombre;
    #String descripcion;
    #String areaAplicacion;
    #String tipo;
    #List<String> objetivos;
    #Soldado soldadoResponsable;
    #List<Soldado> instructores;
    #Date fecha;

    +obtenerLiderResponsable(): Soldado
}

Class ActividadEjercicio extends Actividad {
    -String lugarEntrenamiento;
    -int cantidadMinimaPracticas;
    -String nivelDificultad;
    -List<Actividad> ejerciciosPrevios;
    -String areaCuerpoHumano;
    -double IMCideal;
}

Class ActividadFormacion extends Actividad {
    -String tematicaCapacitacion;
    -String bibliografia;
    -List<String> objetivosAprendizaje;
    -List<String> competencias;
}

Class Insumo {
    -String codigo;
    -String nombreHerramienta;
    -String marca;
    -String modelo;
    -String foto;
    -Date fechaAdquisicion;
    -String tipo;
    -int unidadesAdquiridas;
    -double precio;
    -String estado;
    -boolean esDeConsumo;

    +estaDisponible(): boolean
}

Class ControlInventario {
    -Insumo insumo;
    -int cantidadDisponible;
    -Date fechaRegistro;

    +obtenerEstadoInventario(): String
}

Class RegistroEntregaInsumo {
    -Soldado soldado;
    -Insumo insumo
    -Actividad actividad
    -Date fechaEntrega
    -Date fechaRetorno
}

Class RegistroSalud {
    -Soldado soldado;
    -double peso;
    -int ritmoCardiaco;
    -String condicionSalud;
    -Date fechaRegistro;

    +obtenerCondicionSalud(): String
}

Class CriterioAscenso {
    -String codigo;
    -String descripcion;
    -String tipo;
}


